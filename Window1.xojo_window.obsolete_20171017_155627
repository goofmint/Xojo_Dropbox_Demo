#tag WindowCode
	#tag Event
		Sub Open()
		  App.Dropbox = new Dropbox
		  
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events btnAuth
	#tag Event
		Sub Action()
		  App.Dropbox.appKey = txtClientId.Text
		  
		  htmlAuth.LoadURL(App.Dropbox.getAuthUrl())
		  
		End Sub
	#tag EndEvent
#tag EndEvents
